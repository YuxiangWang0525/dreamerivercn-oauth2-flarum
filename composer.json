{
    "name": "jiangyuan/oauth2-flarum",
    "description": "江愿通行证OAuth2登录插件，允许用户使用江愿通行证账户登录Flarum论坛",
    "type": "flarum-extension",
    "keywords": ["flarum", "oauth2", "authentication", "jiangyuan"],
    "license": "MIT",
    "authors": [
        {
            "name": "江愿文化",
            "email": "support@dreameriver.cn",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@dreameriver.cn",
        "issues": "https://github.com/jiangyuan/flarum-oauth2/issues",
        "source": "https://github.com/jiangyuan/flarum-oauth2"
    },
    "require": {
        "flarum/core": "^1.0",
        "league/oauth2-client": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "JiangYuan\\OAuth\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "江愿通行证OAuth2登录",
            "category": "feature",
            "icon": {
                "name": "fas fa-sign-in-alt",
                "backgroundColor": "#238c59",
                "color": "#fff"
            }
        },
        "flagrow": {
            "discuss": ""
        }
    }
}